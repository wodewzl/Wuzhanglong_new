        <activity android:name=".activity.MyPaurseDetailActivity">
        </activity>
        <activity android:name=".activity.MyPurseActivity">
        </activity>
        <activity android:name=".activity.MyTaskActivity">
        </activity>
        <activity android:name=".activity.MyAttentionActivity">
        </activity>
        <activity
            android:name=".activity.LogoActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppStartLogo"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".activity.MyActivity"/>
        <activity android:name=".activity.MyUserInfoActivity"/>
