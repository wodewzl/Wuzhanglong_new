        <ImageView
            android:layout_width="156dp"
            android:layout_height="95dp"
            android:background="@color/colorAccent"
            android:layout_marginRight="10dp"/>
        <ImageView
            android:layout_width="156dp"
            android:layout_height="95dp"
            android:background="@color/colorAccent"/>
    </LinearLayout>

</LinearLayout>
