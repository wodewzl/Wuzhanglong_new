<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wzl.feifubao">

    <application
        android:name=".application.AppApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeFeiFuBao">

        <!-- 极光推送 -->
        <receiver
            android:name=".receiver.JPushReceiver"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTRATION"/> <!-- Required  用户注册SDK的intent -->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED"/> <!-- Required  用户接收SDK消息的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED"/> <!-- Required  用户接收SDK通知栏信息的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED"/> <!-- Required  用户打开自定义通知栏的intent -->
                <action android:name="cn.jpush.android.intent.ACTION_RICHPUSH_CALLBACK"/> <!-- Optional 用户接受Rich Push Javascript 回调函数的intent -->
                <action android:name="cn.jpush.android.intent.CONNECTION"/> <!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <category android:name="com.wzl.feifubao"/>//换成自己的包名

            </intent-filter>
        </receiver>
        <!-- 极光推送 -->

        <activity
            android:name=".activity.LogoActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppStartLogo"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".activity.JobOffersActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".activity.HouseListActivity" />
        <activity android:name=".activity.HouseAddActivity" />
        <activity android:name=".activity.HouseDetailActivity" />
        <activity android:name=".activity.RegistActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.AddressAddActivity" />
        <activity android:name=".activity.AddressActivity" />
        <activity android:name=".activity.YellowPagesActivity" />
        <activity android:name=".activity.JobOffersDetailActivity" />
        <activity android:name=".activity.OrderSureActivity" />
        <activity android:name=".activity.PayElectricityActivity" />
        <activity android:name=".activity.RateQueryActivity" />
        <activity android:name=".activity.RateExchageActivity" />
        <activity android:name=".activity.PhoneChargeActivity" />
        <activity android:name=".activity.ShopDetailActivity" />
        <activity android:name=".activity.ShopCartActivity" />
        <activity android:name=".activity.MessageActivity" />
        <activity android:name=".activity.MyOverActivity" />
        <activity android:name=".activity.PaymentRecordsActivity" />
        <activity android:name=".activity.WithdrawActivity" />
        <activity android:name=".activity.LifeActivity" />
        <activity android:name=".activity.MyHouseActivity" />
        <activity android:name=".activity.AddressEditActivity" />
        <activity android:name=".activity.AddressSelectActivity" />
        <activity android:name=".activity.OrderActivity" />

        <!-- 支付 -->
        <activity
            android:name=".wxapi.WXPayEntryActivity"
            android:exported="true"
            android:launchMode="singleTop"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" />
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation|screenSize"
            android:exported="false"
            android:screenOrientation="behind"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name="com.alipay.sdk.app.H5AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <!-- 支付 -->
        <activity android:name=".activity.ShopEvaluationActivity" />
        <activity android:name=".activity.UserInfoActivity" />
        <activity android:name=".activity.HouseListOneActivity"></activity>
    </application>

</manifest>